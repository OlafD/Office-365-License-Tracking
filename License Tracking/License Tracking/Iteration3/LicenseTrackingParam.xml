<?xml version="1.0" encoding="utf-8"?>
<LicenseTracking>
  <!-- Url of the SharePoint site collection, where the data should be stored -->
  <Url>https://m365x877207.sharepoint.com/sites/LicenseAlerting</Url>
  <!-- Name of the list in the SharePoint site collection, where the data should be stored -->
  <Listname>License Tracking</Listname>
  <!-- Name of the list in the SharePoint site collection, where the data per Sku should be stored -->
  <ListnamePerSku>License Tracking per Sku</ListnamePerSku>
  <!-- Path for the transcript files -->
  <TranscriptPath>C:\Transcripts</TranscriptPath>
  <!-- from address for sending mails -->
  <MailFrom>admin@m365x877207.onmicrosoft.com</MailFrom>
  <!-- the default receipient for mails -->
  <DefaultReceipient>LicenseAlerting@M365x877207.onmicrosoft.com</DefaultReceipient>
  <!-- Receipients for the notification mails -->
  <Receipients>
    <Receipient>LicenseAlerting@M365x877207.onmicrosoft.com</Receipient>
    <Receipient>admin@m365x877207.onmicrosoft.com</Receipient>
  </Receipients>
  <!-- address for the smtp server to use for sending mails -->
  <SmtpServer>smtp.office365.com</SmtpServer>
  <!-- port number for the smtp server to use for sending mails -->
  <SmtpPort>587</SmtpPort>
</LicenseTracking>
